    "version": "0.2.0",
    "configurations": [

        {
            "name": "Debug script",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
        },
        {
            "name": "Run Django",
            "type": "debugpy",
            "request": "launch",
            "program": "app/manage.py",
            "args": [
                "runserver", "8000"
            ],
            "console": "integratedTerminal",
